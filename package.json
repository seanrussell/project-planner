{
  "name": "sprint-planning",
  "productName": "Project Planner",
  "version": "1.0.0",
  "description": "Electron application for creating projects, backlogs, and sprints.",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanrussell/sprint-planning.git"
  },
  "keywords": [
    "electron",
    "javascript",
    "project",
    "planning",
    "sprint",
    "kanban"
  ],
  "author": "Sean Russell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanrussell/sprint-planning/issues"
  },
  "homepage": "https://github.com/seanrussell/sprint-planning#readme",
  "dependencies": {
    "css-loader": "^5.0.1",
    "dragula": "^3.7.3",
    "marked": "^2.0.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.11.14",
    "nanoid": "^3.1.20",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "electron": "8.2.0",
    "electron-builder": "^22.4.1",
    "electron-webpack": "^2.8.2",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.1",
    "webpack": "~4.42.1"
  },
  "electronWebpack": {
    "commonSourceDirectory": "src/common",
    "title": true,
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "template": "src/renderer/index.html"
    }
  }
}
